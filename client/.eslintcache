ipeReducer, initialState);\r\n\r\n  const { recipes, recipe, loading } = state;\r\n\r\n  // Get all recipes for display\r\n  const getAllRecipes = async () => {\r\n    try {\r\n      dispatch({ type: GET_ALL_RECIPES_REQUEST });\r\n      const { data } = await axios.get('/api/recipes');\r\n      dispatch({ type: GET_ALL_RECIPES_SUCCESS, payload: data });\r\n    } catch (error) {\r\n      console.error(error);\r\n    }\r\n  };\r\n\r\n  // Get individual recipe matching passed in recipe id from url params\r\n  const getSingleRecipe = async id => {\r\n    try {\r\n      dispatch({ type: GET_SINGLE_RECIPE_REQUEST });\r\n      const { data } = await axios.get(`/api/recipes/${id}`);\r\n      dispatch({\r\n        type: GET_SINGLE_RECIPE_REQUEST,\r\n        payload: data,\r\n      });\r\n    } catch (error) {\r\n      console.error(error);\r\n    }\r\n  };\r\n\r\n  const createRecipe = async recipe => {\r\n    try {\r\n    } catch (error) {}\r\n  };\r\n\r\n  return (\r\n    <RecipeContext.Provider\r\n      value={{ recipes, recipe, loading, getAllRecipes, getSingleRecipe }}\r\n    >\r\n      {props.children}\r\n    </RecipeContext.Provider>\r\n  );\r\n};\r\n\r\nexport default RecipeState;\r\n","E:\\Delectable Recipes\\client\\src\\context\\recipes\\recipeContext.js",[],"E:\\Delectable Recipes\\client\\src\\components\\RecipeGroup.js",[],"E:\\Delectable Recipes\\client\\src\\types.js",[],"E:\\Delectable Recipes\\client\\src\\context\\recipes\\recipeReducer.js",["121"],"E:\\Delectable Recipes\\client\\src\\components\\RecipeCard.js",[],"E:\\Delectable Recipes\\client\\src\\DelectableRecipes.js",["122"],"import React, { useContext, useEffect } from 'react';\r\nimport { BrowserRouter as Router, Route, Switch } from 'react-router-dom';\r\nimport UserContext from './context/user/userContext';\r\nimport PrivateRoute from './components/PrivateRoute';\r\nimport Container from 'react-bootstrap/Container';\r\nimport Header from './components/Header';\r\nimport LandingPage from './page